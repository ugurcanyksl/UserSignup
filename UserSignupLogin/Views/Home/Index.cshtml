@model IEnumerable<UserSignupLogin.Models.TBLUserInfo>

@{
    ViewBag.Title = "Index";
}

<h2></h2>
<br />
<br />
<br />
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdUs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UsernameUs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PasswordUs)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdUs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsernameUs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PasswordUs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RePasswordUs)
            </td>
        </tr>
    }
</table>
